&thermal_zones {
	/delete-node/ package-thermal;
	/delete-node/ cpu-thermal;
	/delete-node/ bigcore0-thermal;
	/delete-node/ bigcore2-thermal;
	/delete-node/ littlecore-thermal;
	/delete-node/ center-thermal;
	/delete-node/ gpu-thermal;
	/delete-node/ npu-thermal;

	cpu-thermal {
		polling-delay-passive = <0>;
		polling-delay = <1000>;
		thermal-sensors = <&tsadc 0>;

		trips {
			package-crit {
				temperature = <115000>;
				hysteresis = <0>;
				type = "critical";
			};

			package_fan0: package-fan0 {
				temperature = <40000>;
				hysteresis = <2000>;
				type = "active";
			};



			package_fan2: package-fan2 {
				temperature = <70000>;
				hysteresis = <2000>;
				type = "active";
			};

			package_fan3: package-fan3 {
				temperature = <80000>;
				hysteresis = <2000>;
				type = "active";
			};
			package_fan1: package-fan1 {
				temperature = <60000>;
				hysteresis = <2000>;
				type = "active";
			};
		};

		cooling-maps {
			map0 {
				trip = <&package_fan0>;
				cooling-device = <&pwm_fan 0 2>;
			};

			map1 {
				trip = <&package_fan1>;
				cooling-device = <&pwm_fan 3 4>;
			};

			map2 {
				trip = <&package_fan2>;
				cooling-device = <&pwm_fan 5 6>;
			};

			map3 {
				trip = <&package_fan3>;
				cooling-device = <&pwm_fan 7 7>;
			};

		};
	};

	bigcore0-thermal {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-sensors = <&tsadc 1>;

		trips {
			bigcore0-alert {
				temperature = <85000>;
				hysteresis = <2000>;
				type = "passive";
				phandle = <0x102>;
			};

			bigcore0-crit {
				temperature = <115000>;
				hysteresis = <0>;
				type = "critical";
			};
		};

		cooling-maps {
			map0 {
				trip = <0x102>;
				cooling-device = <&cpu_b0 0xffffffff 0xffffffff
						  &cpu_b1 0xffffffff 0xffffffff>;
			};
		};
	};

	bigcore2-thermal {
		polling-delay-passive = <100>;
		polling-delay = <0>;
		thermal-sensors = <&tsadc 2>;

		trips {
			bigcore2-alert {
